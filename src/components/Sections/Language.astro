---
import Section from "../Section.astro";
import { languages } from "@cv";
---

<Section title="Idiomas">
    <ul>
        {
            languages.map(({ language, fluency }) => {
                return (
                    <article>
                        <p>
                            {language} - {fluency}{" "}
                        </p>
                    </article>
                );
            })
        }
    </ul>
</Section>
